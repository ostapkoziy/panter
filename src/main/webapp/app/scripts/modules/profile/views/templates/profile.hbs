<div class="page-headline">
    <div class="row">
        <div class="medium-6 columns">
            <h4>{{translate "label.profile"}}</h4>
        </div>
        <div class="medium-6 columns text-right">
            <div class="page-controls">
                <a href="#profile/edit"><span class="icon-edit"></span></a>
            </div>
        </div>
    </div>
</div>
<div class="page">
    <div class="row">
        <div class="medium-3 columns">
            <div class="user-avatar">
                <img src="http://ultimate.com.ua/teams/photo/206963135553859cf9aba0e.jpeg"/>
            </div>
        </div>
        <div class="medium-9 columns">
            <div class="user-info">
                <div class="row">
                    <h4>{{firstName}} {{lastName}}</h4>
                </div>
                {{#if dob}}
                    <div class="row">
                        <div class="medium-3 columns">
                            <span>{{translate "label.dob"}}:</span>
                        </div>
                        <div class="medium-9 columns">
                            <span>{{dob}}</span>
                        </div>
                    </div>
                {{/if}}
                <div class="row contacts">
                    <h4>{{translate "label.contacts"}}</h4>
                </div>
                {{#if email}}
                    <div class="row">
                        <div class="medium-3 columns">
                            <span>Email:</span>
                        </div>
                        <div class="medium-9 columns">
                            <span>{{email}}</span>
                        </div>
                    </div>
                {{/if}}
                {{#if skype}}
                    <div class="row">
                        <div class="medium-3 columns">
                            <span>Skype:</span>
                        </div>
                        <div class="medium-9 columns">
                            <span>{{skype}}</span>
                        </div>
                    </div>
                {{/if}}
                {{#if phoneNumber}}
                    <div class="row">
                        <div class="medium-3 columns">
                            <span>{{translate "label.phone"}}:</span>
                        </div>
                        <div class="medium-9 columns">
                            <span>{{phoneNumber}}</span>
                        </div>
                    </div>
                {{/if}}
            </div>
        </div>
    </div>
    <div class="row">
        <div class="medium-12 columns">
            <h4>{{translate "label.main.interests"}}</h4>
        </div>
    </div>
    <div class="row">
        <div class="medium-12 columns">
            <ul class="medium-block-grid-3 small-block-grid-2 interests-widget">
                <li>
                    <div class="item">
                        <div class="interest-image">
                            <img src="http://s3.amazonaws.com/vnn-aws-sites/39/files/2014/01/basketball-366x275.jpg"/>
                        </div>
                        <div class="interest-options">
                            <span>{{translate "hobby.basketball"}}</span>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="item">
                        <div class="interest-image">
                            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSk9lX4KQ9Rvhsm_n3KQqsgs9eq52DvDR__z1ubIMJXD4QyhWhfVw"/>
                        </div>
                        <div class="interest-options">
                            <span>{{translate "hobby.guitar"}}</span>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="item">
                        <div class="interest-image">
                            <img src="data:image/jpeg;base64,"/>
                        </div>
                        <div class="interest-options">
                            <span>{{translate "hobby.bowling"}}</span>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="item">
                        <div class="interest-image">
                            <img src="http://www.mountainsafaritours.com/uploadedFiles/filming.JPG"/>
                        </div>
                        <div class="interest-options">
                            <span>{{translate "hobby.cinema"}}</span>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="item">
                        <div class="interest-image">
                            <img src="https://encrypted-tbn3.gstatic.com/images?q=tbn:ANd9GcR4qmx3HIHANvF8lN4rgcWjhvxDXP2bU4L6PqzG1xfP_Gnex5LBWA"/>
                        </div>
                        <div class="interest-options">
                            <span>{{translate "hobby.sport"}}</span>
                        </div>
                    </div>
                </li>
            </ul>
        </div>
    </div>
</div>